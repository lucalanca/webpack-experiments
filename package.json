{
  "name": "my-shiny-module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/my-shiny-module.js",
  "scripts": {
    "rimraf": "rimraf",
    "webpack": "webpack",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "rimraf dist && webpack --no-minimize && webpack",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:prod": "npm run build:prod && http-server dist --cors -o",
    "test": "karma start"
  },
  "repository": "lucalanca/my-shiny-module",
  "keywords": [],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.2.0",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.0",
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.1",
    "cssnano": "^3.6.2",
    "eslint": "^1.4.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.17.0",
    "http-server": "^0.9.0",
    "husky": "^0.10.1",
    "isparta": "^4.0.0",
    "jasmine-jquery": "^2.1.1",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.1.0",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "node-sass": "^3.7.0",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
